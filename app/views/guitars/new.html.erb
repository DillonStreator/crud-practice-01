<a href="/guitars">back to all guitars</a>
<h1>New guitar</h1>
<%= form_tag "/guitars/create", method: :post do %>
  <p>Name: <input type="text" class="form-control" name="name"></p>
  <p>Strings: <input type="text" class="form-control" name="strings"></p>
  <p>Image: <input type="text" class="form-control" name="image"></p>
  <p>Color: <input type="text" class="form-control" name="color"></p>
  <p>Description: <input type="text" class="form-control" name="description"></p>
  <input type="submit" value="Add guitar">
<% end %>